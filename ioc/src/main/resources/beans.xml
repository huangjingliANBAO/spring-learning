<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/context
       https://www.springframework.org/schema/context/spring-context.xsd">

    <context:component-scan base-package="com.soft1851.spring.ioc.config"/>




   <bean class="com.soft1851.spring.ioc.entity.Hello" id="hello"/>
    <bean class="com.soft1851.spring.ioc.entity.Phone" id="phone">
        <property name="brand" value="小米"/>
        <property name="price" value="1999"/>
    </bean>
    <bean class="com.soft1851.spring.ioc.entity.Student" id="student">
        <property name="id" value="1"/>
        <property name="phone" ref="phone"/>
        <property name="hobbies">
            <list>
                <value>学习</value>
                <value>睡觉</value>
            </list>
        </property>
    </bean>

</beans>